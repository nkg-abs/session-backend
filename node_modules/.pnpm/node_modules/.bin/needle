#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/user12/dev/node/session-backend/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/needle@2.9.1/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/node_modules:/home/user12/dev/node/session-backend/node_modules:/home/user12/dev/node/node_modules:/home/user12/dev/node_modules:/home/user12/node_modules:/home/node_modules:/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/node_modules/needle/bin/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/node_modules/needle/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/user12/dev/node/session-backend/node_modules/.pnpm/needle@2.9.1/node_modules/needle/bin/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/needle@2.9.1/node_modules/needle/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/needle@2.9.1/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/node_modules:/home/user12/dev/node/session-backend/node_modules:/home/user12/dev/node/node_modules:/home/user12/dev/node_modules:/home/user12/node_modules:/home/node_modules:/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/node_modules/needle/bin/node_modules:/home/user12/dev/node/session-backend/node_modules/.pnpm/node_modules/needle/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../needle/bin/needle" "$@"
else
  exec node  "$basedir/../needle/bin/needle" "$@"
fi
